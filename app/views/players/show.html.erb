  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="shortcut icon" href="assets/images/logo.png" type="image/x-icon">
  <meta name="description" content="">
  
  <link href="https://fonts.googleapis.com/css?family=Play" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Play:700" rel="stylesheet">
  <%= stylesheet_link_tag "material.css" %>
  <%= stylesheet_link_tag "tether.min.css" %>
  <%= stylesheet_link_tag "bootstrap.min.css" %>
  <%= stylesheet_link_tag "style.css" %>
  <%= stylesheet_link_tag "mbr-additional.css" %>
  
</head>
<body>

<section class="mbr-section mbr-section-hero mbr-section-full" id="header5-f" style="background-image: url('http://ddragon.leagueoflegends.com/cdn/img/champion/splash/<%= @player.c1 %>_0.jpg');background-size: cover; height: 717px;">

    

    <div class="mbr-table-cell">

        <div class="container">
            <div class="row">
                <div class="mbr-section col-md-10">

                    <h1 class="mbr-section-title display-1"><%= @player.p1.capitalize %></h1>
                    <p class="mbr-section-lead lead"></p>
                    
                </div>
            </div>
        </div>
    </div>

</section>

<section class="mbr-section mbr-section-hero mbr-section-full mbr-parallax-background mbr-section-with-arrow" id="header5-g" style="background-image: url('http://ddragon.leagueoflegends.com/cdn/img/champion/splash/<%= @player.c2 %>_0.jpg');background-size: cover;background-size: cover; height: 717px;background-attachment: fixed;">

    

    <div class="mbr-table-cell">

        <div class="container">
            <div class="row">
                <div class="mbr-section col-md-10 col-md-offset-2 text-xs-right">

                    <h1 class="mbr-section-title display-1"><%= @player.p2.capitalize %></h1>
                    
                    
                </div>
            </div>
        </div>
    </div>

</section>

<section class="mbr-section mbr-section-hero mbr-section-full mbr-section-with-arrow" id="header5-h">

    

  <div class="mbr-table-cell">

    <div class="container">

      <div class="row">

        <div class="mbr-section col-md-10 col-md-offset-1 text-xs-center" style="min-height:100%">
<!-- Si p1 a un meilleur winrate, alors .. -->
<%= if Player.determine_champion_probability(@player.p1, @player.c1)>Player.determine_champion_probability(@player.p2, @player.c2)
    #if win
    wonned

    else
    #if lost
    loosed
                                      end %>
        </div>
      </div>
    </div>
  </div>

</section>

<section class="mbr-section article" id="msg-box3-i" style="background-color: rgb(242, 242, 242); padding-top: 80px; padding-bottom: 80px;">

    
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2 text-xs-center">
                
                
                <div><%= link_to "Nouveau combat ?", players_path %></div>
            </div>
        </div>
    </div>

</section>

<footer class="mbr-small-footer mbr-section mbr-section-nopadding" id="footer1-2" style="background-color: rgb(50, 50, 50); padding-top: 1.75rem; padding-bottom: 1.75rem;">
    
